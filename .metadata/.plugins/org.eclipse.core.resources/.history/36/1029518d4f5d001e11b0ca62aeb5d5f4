package test;

public class StringExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		String name1 = "Nikhil";
		String name2 = "Neha";
		
		System.out.println("String: " + name1);
		System.out.println("Length of String: " + name1.length());
		System.out.println("First char: " + name1.charAt(0));
		System.out.println("Last char: " + name1.charAt(name1.length()-1));
		System.out.println("Uppercase: " + name1.toUpperCase());
		System.out.println("Lowercase: " + name2.toLowerCase());
	}

}
