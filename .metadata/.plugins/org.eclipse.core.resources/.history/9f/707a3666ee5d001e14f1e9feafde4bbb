package test;

public class LoopingExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

//********************************************//		
		int num1 = 1;
		
		while(num1<11) {
			System.out.print(num1);
			num1 = num1 + 1;
		}
		
		System.out.println("End of while loop");
		
//*********************************************//		
		int num2 = 10;
		
		while(num2>0) {
			System.out.print(num2);
			num2 = num2-1;
		}
		System.out.println("End of while loop");
//*********************************************//		
		for (int a = 1; a <= 10; a++) {
			System.out.print(a);
		}

	}

}