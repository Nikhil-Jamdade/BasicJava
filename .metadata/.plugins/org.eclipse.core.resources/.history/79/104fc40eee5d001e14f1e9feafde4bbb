package test;

public class LoopingExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

//********************************************//		
		int num = 1;
		
		while(num<11) {
			System.out.println(num);
			num = num + 1;
		}
		
		System.out.println("End of while loop");
		
//*********************************************//		
		for (int a = 1; a <= 10; a++) {
			System.out.print(a);
		}

	}

}